{
  "name": "codama-dart",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/index.node.cjs",
  "module": "./dist/index.node.mjs",
  "types": "./dist/index.d.ts",
  "type": "module",
  "files": [
    "./dist/templates",
    "./dist/types",
    "./dist/index.*"
  ],
  "sideEffects": false,
  "keywords": [
    "solana",
    "framework",
    "standard",
    "renderers",
    "dart",
    "client"
  ],
  "scripts": {
    "build": "rimraf dist && pnpm build:src && pnpm build:types",
    "build:src": "tsup src/index.ts --format cjs,esm --no-dts --platform node --config tsup.config.node.ts",
    "build:types": "tsc --build tsconfig.declarations.json",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "pnpm test:types && pnpm test:treeshakability && pnpm test:node && pnpm test:e2e && pnpm test:exports",
    "test:e2e": "bash ./e2e/test.sh",
    "test:exports": "node ./test/exports/module.mjs && node ./test/exports/commonjs.cjs",
    "test:node": "zx ./scripts/test-unit.mjs node",
    "test:treeshakability": "zx ./scripts/test-treeshakability.mjs",
    "test:types": "zx ./scripts/test-types.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LimeChain/codama-dart.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LimeChain/codama-dart/issues"
  },
  "homepage": "https://github.com/LimeChain/codama-dart#readme",
  "devDependencies": {
    "@codama/nodes-from-anchor": "^1.2.8",
    "@types/node": "^24.5.1",
    "@types/nunjucks": "^3.2.6",
    "agadoo": "^3.0.0",
    "browserslist-to-esbuild": "^2.1.1",
    "codama": "^1.3.7",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "zx": "^8.8.1"
  },
  "dependencies": {
    "@codama/errors": "^1.3.4",
    "@codama/nodes": "^1.3.4",
    "@codama/renderers-core": "^1.1.0",
    "@codama/visitors-core": "^1.3.4",
    "nunjucks": "^3.2.4"
  },
  "browserslist": [
    "supports bigint and not dead",
    "maintained node versions"
  ]
}
