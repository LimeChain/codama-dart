{
  "name": "codama-dart",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/index.node.cjs",
  "module": "./dist/index.node.mjs",
  "types": "./dist/index.d.ts",
  "type": "module",
  "files": [
      "./dist/templates",
      "./dist/types",
      "./dist/index.*"
  ],
  "sideEffects": false,
  "keywords": [
    "solana",
    "framework",
    "standard",
    "renderers",
    "dart",
    "client"
  ],
  "scripts": {
    "build": "rimraf dist && pnpm build:src && pnpm build:types",
    "build:src": "tsup src/index.ts --format cjs,esm --no-dts --platform node --config tsup.config.node.ts",
    "build:types": "tsc --build tsconfig.declarations.json",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:e2e": "sh ./e2e/test.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LimeChain/codama-dart.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LimeChain/codama-dart/issues"
  },
  "homepage": "https://github.com/LimeChain/codama-dart#readme",
  "devDependencies": {
    "@types/node": "^24.3.1",
    "@types/nunjucks": "^3.2.6",
    "browserslist-to-esbuild": "^2.1.1",
    "rimraf": "^5.0.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@codama/errors": "^1.3.4",
    "@codama/nodes": "^1.3.4",
    "@codama/renderers-core": "^1.1.0",
    "@codama/visitors-core": "^1.3.4",
    "nunjucks": "^3.2.4"
  },
  "browserslist": [
    "supports bigint and not dead",
    "maintained node versions"
  ]
}
